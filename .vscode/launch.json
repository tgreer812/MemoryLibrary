{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Module List",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": ["--module-list", "--pid", "372"]
        },
        {
            "name": "Scan",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": [
                "--scan",
                "--pid",
                "372",
                "--value-type",
                "int",
                "--value",
                "10",
                "--config",
                "{\"found_addresses\":[]}"
            ]
        },
        {
            "name": "Python: Client",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/PythonAGent/client.py",
            "args": ["--host", "127.0.0.1", "--port", "3001", "--debug"],
            "console": "integratedTerminal"
        }
        
        
    ]
}
